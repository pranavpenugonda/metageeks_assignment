#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/upgrader/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/upgrader/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/husky@4.3.8/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/upgrader/bin.js" "$@"
else
  exec node  "$basedir/../../lib/upgrader/bin.js" "$@"
fi
