#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/bin/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/bin/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/1.3React.js/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/Part1/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/Assignment/node_modules:/home/workspace/nodejs/coding-practices/Metageeks Assignment/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/resolve" "$@"
else
  exec node  "$basedir/../../bin/resolve" "$@"
fi
